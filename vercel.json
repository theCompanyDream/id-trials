{
	"version": 2,
	"builds": [
	  {
		"src": "api/index.go",
		"use": "@vercel/go"
	  },
	  {
		"src": "apps/frontend/package.json",
		"use": "@vercel/static-build",
		"config": { "distDir": "dist/frontend" }
	  },
	 {
		"src": "api/analytics/*.py",
		"use": "@vercel/python"
	  }
	],
	"routes": [
      {
		"src": "/api/analytics/(.*) ",
		"dest": "api/analytics/$1"
      },
	  {
		"src": "/api/.*",
		"dest": "api/index.go"
	  },
	  {
		"src": "/(.*)",
		"dest": "apps/frontend/$1"
	  }
	]
  }