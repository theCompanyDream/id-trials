{
	"version": 2,
	"builds": [
	  {
		"src": "api/*.go",
		"use": "@vercel/go"
	  },
	  {
		"src": "apps/frontend/package.json",
		"use": "@vercel/static-build",
		"config": { "distDir": "dist/frontend" }
	  }
	],
	"routes": [
	  {
		"src": "/api/(.*)",
		"dest": "api/$1"
	  },
	  {
		"src": "/(.*)",
		"dest": "apps/frontend/dist/frontend/$1"
	  }
	]
  }